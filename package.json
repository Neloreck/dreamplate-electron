{
  "name": "dreamplate",
  "version": "0.0.0",
  "private": true,
  "repository": "https://github.com/neloreck/dreamplate",
  "description": "Dreamplate boilerplate.",
  "author": {
    "name": "Igor Syrotenko",
    "email": "neloreck@gmail.com>"
  },
  "license": "ISC",
  "scripts": {},
  "dependencies": {
    "electron": "^22.3.21",
    "react": "^18.1.0",
    "react-dom": "^18.1.0"
  },
  "devDependencies": {
    "@types/jest": "^26.0.23",
    "@types/node": "^14.14.7",
    "@types/react": "^18.0.12",
    "@types/react-dom": "^18.0.5",
    "cross-env": "^7.0.3",
    "electron-builder": "^23.0.3",
    "pnpm": "^7.2.1",
    "ts-node": "^10.8.1",
    "typescript": "^4.7.3"
  },
  "build": {
    "productName": "DreamplateElecton",
    "appId": "org.nck.DreamplateElecton",
    "asar": true,
    "asarUnpack": "**\\*.{node,dll}",
    "files": [
    ],
    "mac": {
      "target": {
        "target": "default",
        "arch": [
          "arm64",
          "x64"
        ]
      },
      "type": "distribution",
      "hardenedRuntime": true,
      "entitlements": "target/dist/entitlements.mac.plist",
      "entitlementsInherit": "target/dist/entitlements.mac.plist",
      "gatekeeperAssess": false
    },
    "dmg": {
      "contents": [
        {
          "x": 130,
          "y": 220
        },
        {
          "x": 410,
          "y": 220,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "win": {
      "target": [
        "nsis",
        "portable"
      ]
    },
    "linux": {
      "target": [
        "AppImage",
        "tar.xz"
      ],
      "icon": "target/dist/build_resources/icon.png",
      "category": "Development"
    },
    "directories": {
      "app": "target/dist",
      "buildResources": "target/dist/build_resources",
      "output": "target/release"
    },
    "extraResources": [
      {
        "from": "target/dist/",
        "to": "app/",
        "filter": ["!**/entitlements.mac.plist"]
      }
    ]
  }
}
